{
  "name": "ng-jest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --no-cache",
    "test:cov": "jest --no-cache --coverage",
    "test:watch": "jest --no-cache --watch",
    "lint": "ng lint"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest.config.ts"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "./src/app/**/*.ts",
      "!./src/app/**/*.module.ts"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ]
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jest": "^24.0.11",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jest": "^24.7.1",
    "jest-preset-angular": "^7.1.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
